# E2E Test Environment Configuration
# This file contains environment variables specifically for E2E testing

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_BASE_URL=http://localhost:5000

# Test Mode Flag
VITE_TEST_MODE=true

# Enable CORS for test requests
VITE_ENABLE_CORS=true

# Disable analytics and tracking in tests
VITE_ENABLE_ANALYTICS=false

# Test Database Configuration
VITE_USE_TEST_DB=true

# Logging Configuration
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# Feature Flags for Testing
VITE_ENABLE_EXPERIMENTAL_FEATURES=false
VITE_ENABLE_DEBUG_MODE=true

# Authentication Configuration
VITE_AUTH_TOKEN_EXPIRY=3600
VITE_SESSION_TIMEOUT=1800

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_ERROR_REPORTING=false

